    @section scripts {
        <script src="~/Scripts/jquery-1.9.1.min.js"></script>
        <script src="~/Content/bootstrap/js/bootstrap.min.js"></script>
    }
    <link href="~/Content/Site.css" type="text/css" rel="stylesheet" />
